{
  "name": "@dennis273/geet",
  "version": "1.0.0-alpha.3",
  "description": "A command line tool to help handle git repo.",
  "repository": "https://github.com/Dennis273/geet.git",
  "author": "Dennis273 <dennisc695@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --bundle --outfile=./dist/index.js",
    "prepare": "husky install"
  },
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@types/node": "18.6.4",
    "commander": "2.20.3",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "esbuild": "0.14.53"
  },
  "bin": {
    "geet": "./dist/index.js"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
